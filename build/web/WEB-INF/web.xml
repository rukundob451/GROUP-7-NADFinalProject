<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>com.Servlets.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>com.Servlets.AdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddToCart</servlet-name>
        <servlet-class>com.Servlets.AddToCart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuantityServlet</servlet-name>
        <servlet-class>com.Servlets.QuantityServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaleServlet</servlet-name>
        <servlet-class>com.Servlets.SaleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Allocate</servlet-name>
        <servlet-class>com.Servlets.Allocate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Reallocate</servlet-name>
        <servlet-class>com.Servlets.Reallocate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LikeServlet</servlet-name>
        <servlet-class>com.Servlets.LikeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CalculateProducts</servlet-name>
        <servlet-class>com.Servlets.CalculateProducts</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddProducts</servlet-name>
        <servlet-class>com.Servlets.AddProducts</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLogout</servlet-name>
        <servlet-class>com.Servlets.AdminLogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>com.Servlets.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/AdminServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddToCart</servlet-name>
        <url-pattern>/AddToCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuantityServlet</servlet-name>
        <url-pattern>/QuantityServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaleServlet</servlet-name>
        <url-pattern>/SaleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Allocate</servlet-name>
        <url-pattern>/Allocate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Reallocate</servlet-name>
        <url-pattern>/Reallocate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LikeServlet</servlet-name>
        <url-pattern>/LikeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CalculateProducts</servlet-name>
        <url-pattern>/CalculateProducts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddProducts</servlet-name>
        <url-pattern>/AddProducts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogout</servlet-name>
        <url-pattern>/AdminLogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
